		__proto__: null,
		getPrintBordersCss: getPrintBordersCss,
		init: init$v,
		print: print,
		removePrintInnerBorders: removePrintInnerBorders,
		showBorders: showBorders
	});

	var readyZoom = {};
	function init$u(view) {
	  view.attachEvent("onUndo", function (action, row, column, value) {
	    if (action == "zoom") {
	      zoom$3(view, value);
	    }
	  });
