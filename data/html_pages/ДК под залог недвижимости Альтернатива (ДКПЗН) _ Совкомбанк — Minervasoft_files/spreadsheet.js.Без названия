	            var data = view.ranges.serialize(view.getActiveSheet()).map(function (r) {
	              return {
	                name: r[0],
	                value: r[1]
	              };
	            });
	            if (!data.length) return false;
	            var list = this.getList();
	            list.clearAll();
	            list.parse(data);
