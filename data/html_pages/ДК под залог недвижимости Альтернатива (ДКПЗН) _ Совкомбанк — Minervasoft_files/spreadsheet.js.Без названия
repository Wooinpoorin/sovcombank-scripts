	  }
	}, webix.ui.scrollview);

	function init$A(view) {
	  //parsing initial data
	  view.attachEvent("onDataParse", function (data) {
	    return _parse$2(view, data);
	  });
	  view.attachEvent("onDataSerialize", function (data, config) {
