	  grid.attachEvent("onHeaderClick", function (id, e) {
	    if (id.column == "rowId") {
	      selectAll(view);
	      return;
	    }
	    var headerClick = new Date();
	    var dblClick = headerClick - lastHeaderClick <= 300;
	    if (dblClick) {
	      adjustColumn(id.column, view);
	    } else {
	      lastHeaderClick = headerClick;
	      if (e.shiftKey) {
	        selectColumns(id.column, view);
	      } else {
