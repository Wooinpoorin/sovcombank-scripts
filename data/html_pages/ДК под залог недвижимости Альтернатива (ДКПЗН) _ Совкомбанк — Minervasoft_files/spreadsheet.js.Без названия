	function splitCell(row, column, page, silent) {
	  var isActivePage = !page || page == this.getActiveSheet();
	  if (row && column) {
	    removeSpan(this, {
	      row: row,
	      column: column
	    }, page);
	  } else if (isActivePage) {
	    group.set(function () {
	      this.eachSelectedCell(function (cell) {
	        removeSpan(this, cell);
	      });
	    }, this);
	  }
	  if (!silent && isActivePage) this.refresh();
	}

